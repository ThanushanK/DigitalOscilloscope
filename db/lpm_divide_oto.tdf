FUNCTION abs_divider_5dg (denominator[31..0], numerator[41..0])
RETURNS ( quotient[41..0], remainder[31..0]);

--synthesis_resources = lut 1043 
SUBDESIGN lpm_divide_oto
( 
	denom[31..0]	:	input;
	numer[41..0]	:	input;
	quotient[41..0]	:	output;
	remain[31..0]	:	output;
) 
VARIABLE 
	divider : abs_divider_5dg;
	numer_tmp[41..0]	: WIRE;

BEGIN 
	divider.denominator[] = denom[];
	divider.numerator[] = numer_tmp[];
	numer_tmp[] = numer[];
	quotient[] = divider.quotient[];
	remain[] = divider.remainder[];
END;
